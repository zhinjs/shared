{
  "name": "@zhinjs/shared",
  "version": "0.0.6",
  "description": "知音共享模块",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "node .",
    "compile": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build": "npm run compile && dtsc --project tsconfig.dtsc.json",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "pub": "npm publish --access public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhinjs/shared.git"
  },
  "keywords": [
    "ts",
    "dev",
    "template"
  ],
  "author": "凉菜",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhinjs/shared/issues"
  },
  "homepage": "https://github.com/zhinjs/shared#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "latest",
    "dtsc": "^2.3.0",
    "ts-node-dev": "latest",
    "tsc-alias": "latest",
    "tsconfig-paths": "latest",
    "typescript": "latest"
  },
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "/lib/**/LICENSE"
  ],
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
